{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "plugins": [
    "prettier",
    "import",
    "node"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:node/recommended",
    "plugin:import/errors",
    "prettier"
  ],
  "rules": {
    "prettier/prettier": "error",
    "no-console": "off"
  }
}
